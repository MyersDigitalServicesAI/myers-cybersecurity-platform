# PostgreSQL database connection URL (format: postgres://user:password@host:port/dbname)
DATABASE_URL=postgres://mockuser:mockpassword@mockhost:5432/mockdb

# Stripe API Keys
STRIPE_SECRET_KEY=sk_test_mocksecretkey
STRIPE_PUBLISHABLE_KEY=pk_test_mockpublishablekey
STRIPE_WEBHOOK_SECRET=whsec_mockwebhooksecret

# SendGrid email API key
SENDGRID_API_KEY=SG.mockapikey

# SMTP settings (used by SendGrid)
SMTP_SERVER=smtp.sendgrid.net
SMTP_PORT=587
SMTP_USER=apikey
SMTP_API_KEY=SG.mockapikey

# The default sender email address for transactional emails
FROM_EMAIL=support@examplecybersecurity.com
SENDER_EMAIL=support@examplecybersecurity.com

# Domain and admin email
DOMAIN=https://examplecybersecurity.onrender.com
APP_URL=https://examplecybersecurity.onrender.com
SETUP_ADMIN_EMAIL=admin@examplecybersecurity.com

# Environment: production, development, etc.
ENVIRONMENT=production

# Security keys
FERNET_KEY=mockfernetkey12345678901234567890123456=
CAPTCHA_SECRET_KEY=mockcaptchasecretkey

# Rate limiting settings
RATE_LIMIT_CALLS=5
RATE_LIMIT_PERIOD=60

